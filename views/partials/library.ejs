<aside class="right-side">
    <section class="content-header">
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">My Library</li>
        </ol>
    </section>
    <table class="table" ng-controller="ProfileController" ng-init="init()">
        <thead>
            <tr>
                <th></th>
                <th>Song</th>
                <th>Artist</th>
            </tr>
        </thead>
        <tbody>
            <tr data-ng-repeat="song in usersongs">
                <td></td>
                <td><a href="/viewsong/{{song._id}}">{{ song.title }}</a></td>
                <td><a href="/artist/{{song.artist}}">{{ song.artist }}</a></td>
            </tr>
        </tbody>
    </table>
</aside>