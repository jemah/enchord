<aside class="right-side" ng-controller="BandViewController" ng-init="init()">
    <section class="content-header">
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">This Band Name</li>
            <li class="active">Active Folder Name</li>
        </ol>
    </section><br>
    <table class="table">
        <thead>
            <tr>
                <th></th>
                <th>Song</th>
                <th>Artist</th>
                <th>Genre</th>
            </tr>
        </thead>
        <tbody>
        <tr data-ng-repeat="song in usersongs">
                <td></td>
                <td><a href="/viewsong/{{song._id}}">{{ song.title }}</a></td>
                <td><a href="/artist/{{song.artist}}">{{ song.artist }}</a></td>
                <td>{{ song.genre }}</td>
            </tr>
        </tbody>
    </table>
</aside>